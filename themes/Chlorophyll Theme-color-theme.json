			"entity.other.keyframe-offset",
			"markup.heading",
			"markup.underline.link"
		  ],
		  "settings": {
			"foreground": "#9479ce"
		  }
		},
		{
		  "name": "Accent2",
		  "scope": [
			"string",
			"markup.inserted",
			"markup.raw",
			"constant"
		  ],
		  "settings": {
			"foreground": "#68bb9f"
		  }
		},
		{
		  "name": "Accent3",
		  "scope": [
			"markup.changed",
			"entity"
		  ],
		  "settings": {
			"foreground": "#ccba6d"
		  }
		},
		{
		  "name": "Accent5",
		  "scope": [

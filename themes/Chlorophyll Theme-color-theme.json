	 "tokenColors": [
		{
		  "name": "Accent1",
		  "scope": [
			"keyword",
			"storage",
			"support",
			"entity.name.tag",
			"variable.language",
			"keyword.control.flow",
			"storage.modifier",
			"keyword.operator",
